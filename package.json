{
  "name": "api-example",
  "version": "1.0.0",
  "description": "API feita em GraphQL com interface VueJs e utilizando Electron",
  "main": "src/main.js",
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run vue:dev\" \"npm run electron:load\"",
    "start:prod": "concurrently \"npm run server\" \"npm run vue:dev\" \"npm run electron:load\"",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run vue:dev\" \"npm run electron:load\"",
    "server": "node src/server.js",
    "server:dev": "nodemon src/server.js",
    "electron:load": "electron src/electron.js",
    "vue:dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "vue:build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasVmigotto/api-example.git"
  },
  "keywords": [
    "Electron",
    "API",
    "GraphQL",
    "MoongoDB",
    "Express"
  ],
  "author": "Lucas Vidor Migotto <lucasmigotto@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasVmigotto/api-example/issues"
  },
  "homepage": "https://github.com/LucasVmigotto/api-example#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^2.0.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "standard": "^11.0.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "graphql-tools": "^3.0.2",
    "jquery": "^3.3.1",
    "mongoose": "^5.1.4",
    "path": "^0.12.7",
    "vue": "^2.5.11",
    "vuetify": "^1.0.19"
  }
}
